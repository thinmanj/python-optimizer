# Python Optimizer Environment Configuration
# Copy this file to .env and adjust values as needed

# JIT Compilation Settings
PYTHON_OPTIMIZER_JIT_CACHE=1
PYTHON_OPTIMIZER_JIT_CACHE_DIR=~/.python_optimizer/cache
PYTHON_OPTIMIZER_JIT_COMPILE_TIMEOUT=30

# Profiling Settings
PYTHON_OPTIMIZER_PROFILE=1
PYTHON_OPTIMIZER_PROFILE_DIR=./profiles
PYTHON_OPTIMIZER_PROFILE_THRESHOLD=0.001

# Parallel Processing
PYTHON_OPTIMIZER_PARALLEL=1
PYTHON_OPTIMIZER_MAX_WORKERS=4

# Optimization Settings
PYTHON_OPTIMIZER_AGGRESSIVENESS=2
PYTHON_OPTIMIZER_SPECIALIZE=0

# Development Settings
PYTHON_OPTIMIZER_DEBUG=0
PYTHON_OPTIMIZER_LOG_LEVEL=INFO

# Benchmarking Settings
PYTHON_OPTIMIZER_BENCHMARK_RUNS=5
PYTHON_OPTIMIZER_BENCHMARK_WARMUP=3
